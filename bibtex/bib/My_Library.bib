  booktitle = {2019 {{IEEE}}/{{RSJ International Conference}} on {{Intelligent Robots}} and {{Systems}} ({{IROS}})},
  author = {Abbasi, Bahareh and Monaikul, Natawut and Rysbek, Zhanibek and Eugenio, Barbara Di and Zefran, Milos},
