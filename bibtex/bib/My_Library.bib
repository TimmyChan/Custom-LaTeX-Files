@inproceedings{alozieChallengesDesigningInstructionally18,
  title = {Challenges in {{Designing Instructionally Supportive Science Assessments Using Culturally Relevant Principles}} for {{Diverse Students}}},
  booktitle = {Bringing Equity to Science Education through Curriculum, Assessment, and Community-Based Experimental Learning},
  author = {Alozie, Nonye and Madden, Krystal Nacole Alberta and Zaidi, Sania Zahra and Pennock, Phyllis Haugabook and Harris, Christopher J.},
  date = {0018-04-13},
  publisher = {{AERA}},
  location = {{New York, NY}},
  abstract = {The goal of this paper is to highlight tensions and challenges that arise in designing science assessment tasks that promote cultural relevance for historically underrepresented students (e.g., low socioeconomic status (SES) and ethnically and culturally underrepresented students). We present our process for developing equitable and instructionally supportive assessment tasks for multicultural students, resulting assessment tasks, and challenges associated with attending to cultural relevance (Ladson-Billings, 1995; 2014) in assessment design. We ask the question: “How can we achieve equity and fairness in the design of assessment tasks that incorporate tenets of culturally relevant pedagogy (CRP) without introducing construct irrelevant variance?” We present an equity and fairness assessment design framework and explore the infusion of CRP in our assessment design process to determine the extent to which culturally relevant pedagogy can be applied. This work is in response to the adoption of the Next Generation Science Standards (NGSS), where teachers are faced with how to assess students from diverse backgrounds in science concepts, themes, and practices. Our design work is being carried out in the context of a multi-institutional collaborative, where we recognized a need for a clear and efficient approach to designing assessment tasks that can inform instruction in increasingly diverse classrooms.},
  eventtitle = {Annual Meeting of the {{American Educational Research Association}}},
  file = {C\:\\Users\\EVKae\\Google Drive\\Academic\\Zotero\\storage\\7CFSID49\\Alozie et al. - 2018 - Challenges in Designing Instructionally Supportive.pdf}
}

