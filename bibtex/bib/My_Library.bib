@article{richardsReframingResponsivenessChallenge2020,
  title = {Reframing the {{Responsiveness Challenge}}: {{A Framing}}-{{Anchored Explanatory Framework}} to {{Account}} for {{Irregularity}} in {{Novice Teachers}}’ {{Attention}} and {{Responsiveness}} to {{Student Thinking}}},
  shorttitle = {Reframing the {{Responsiveness Challenge}}},
  author = {Richards, Jennifer and Elby, Andrew and Luna, Melissa J. and Robertson, Amy D. and Levin, Daniel M. and Nyeggen, Colleen G.},
  date = {2020-04-02},
  journaltitle = {Cognition and Instruction},
