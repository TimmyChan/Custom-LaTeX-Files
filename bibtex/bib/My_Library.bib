@article{tylerRestorativeJusticeProcedural2006,
  title = {Restorative {{Justice}} and {{Procedural Justice}}: {{Dealing}} with {{Rule Breaking}}},
  shorttitle = {Restorative {{Justice}} and {{Procedural Justice}}},
  author = {Tyler, Tom R.},
  date = {2006},
  journaltitle = {Journal of Social Issues},
  volume = {62},
