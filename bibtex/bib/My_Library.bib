  title = {Development and Initial Validation of the {{Literature Epistemic Cognition Scale}} ({{LECS}})},
  author = {Yukhymenko-Lescroart, Mariya A. and Briner, Stephen W. and Magliano, Joseph P. and Lawless, Kimberly and Burkett, Candice and McCarthy, Kathryn S. and Lee, Carol D. and Goldman, Susan R.},
